{
  "name": "@ballerina/diagram",
  "version": "0.983.1-SNAPSHOT",
  "description": "Provides a set of react components to draw Ballerina Diagrams.",
  "keywords": [
    "ballerina",
    "ballerina-lang",
    "composer"
  ],
  "repository": "ballerina-platform/ballerina-lang",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib && rimraf coverage",
    "prebuild": "npm run clean && npm run lint",
    "compile": "tsc -p ./ && copyfiles resources/bbe-asts/* lib-ts",
    "build": "npm run compile",
    "watch": "tsc -watch -p ./",
    "test": "jest",
    "test-coverage": "npm run test -- --coverage",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "lint": "tslint -t stylish --project .",
    "start-ws-ls": "npm run compile && node lib-ts/tools/start-ws-lang-server.js",
    "gen-bbe-asts": "npm run compile && node lib-ts/tools/update-bbe-asts.js",
    "gen-models": "npm run compile && node lib-ts/tools/generate-models.js"
  },
  "author": "ballerina.io",
  "license": "Apache-2.0",
  "peerDependencies": {
    "react-dom": "^16.2.0",
    "react": "^16.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@storybook/addon-info": "^4.0.4",
    "@storybook/react": "^4.0.4",
    "@types/common-tags": "^1.4.0",
    "@types/jest": "^23.3.9",
    "@types/react": "16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-test-renderer": "^16.0.3",
    "@types/rimraf": "^2.0.2",
    "@types/storybook__react": "^4.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.4",
    "common-tags": "^1.8.0",
    "copyfiles": "^2.1.0",
    "jest": "^23.6.0",
    "prettier-tslint": "^0.4.0",
    "react": "^16.2.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "^16.6.1",
    "require-context": "^1.1.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.3.0",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6",
    "vscode-uri": "^1.0.6"
  },
  "dependencies": {
    "@ballerina/ast-model": "file:../ast-model",
    "@ballerina/lang-service": "file:../lang-service",
    "@ballerina/theme": "file:../theme",
    "axios": "^0.16.2",
    "jquery": "^3.3.1",
    "jsplumb": "2.3.5",
    "log4javascript": "^1.4.15",
    "prettier": "^1.15.2",
    "react-autosuggest": "^9.0.1",
    "react-custom-scrollbars": "^4.1.2",
    "react-test-renderer": "^16.6.1",
    "react-transition-group": "^1.1.2",
    "semantic-ui-react": "^0.81.3",
    "uuid": "^3.1.0",
    "winston": "^3.1.0"
  }
}
